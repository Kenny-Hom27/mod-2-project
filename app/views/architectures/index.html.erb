<h1 class="welcome">All Architectural Works</h1>
<div class="add_item">
  <%= button_to "Add an Architectural Work", new_architecture_path, method: :get %>
</div>
<div class="items">
  <ul>
  <% @architectures.each do |architecture| %>
    <li class="item"><%= link_to architecture.name.titleize, architecture_path(architecture) %></li>
  <% end %>
  </ul>
</div>
